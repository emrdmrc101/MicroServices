{
  "Logging": {
    "Serilog": {
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Information",
          "System": "Warning"
        }
      }
    },
    "LoggerType": "Serilog"
  },
  "Jwt": {
    "securityKey": "DEBEEBA-A234-479F-A94C-BF2E1373B70B",
    "validateIssuerSigningKey": true,
    "validateIssuer": false,
    "validIssuer": "edemirci",
    "validateAudience": true,
